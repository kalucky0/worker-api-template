{
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.tsx": "$(capture).module.scss, $(capture).scss, $(capture).module.css, $(capture).css",
        "wrangler.jsonc": "*.config.ts, *.config.js, *.config.mjs, *.config.json, worker-configuration.d.ts",
    },
    "files.exclude": {
        "**/logs": true,
        "**/_.log": true,
        "**/npm-debug.log_": true,
        "**/yarn-debug.log*": true,
        "**/yarn-error.log*": true,
        "**/lerna-debug.log*": true,
        "**/.pnpm-debug.log*": true,
        "**/report.[0-9]_.[0-9]_.[0-9]_.[0-9]_.json": true,
        "**/pids": true,
        "**/_.pid": true,
        "**/_.seed": true,
        "**/\\*.pid.lock": true,
        "**/lib-cov": true,
        "**/coverage": true,
        "**/\\*.lcov": true,
        "**/.nyc_output": true,
        "**/.grunt": true,
        "**/bower_components": true,
        "**/.lock-wscript": true,
        "build/Release": true,
        "**/node_modules/": true,
        "**/jspm_packages/": true,
        "**/web_modules/": true,
        "**/*.tsbuildinfo": true,
        "**/.npm": true,
        "**/.eslintcache": true,
        "**/.stylelintcache": true,
        "**/.rpt2_cache/": true,
        "**/.rts2_cache_cjs/": true,
        "**/.rts2_cache_es/": true,
        "**/.rts2_cache_umd/": true,
        "**/.node_repl_history": true,
        "**/\\*.tgz": true,
        "**/.yarn-integrity": true,
        "**/.cache": true,
        "**/.parcel-cache": true,
        "**/.next": true,
        "**/out": true,
        "**/.nuxt": true,
        "**/dist": true,
        "**/.cache/": true,
        ".vuepress/dist": true,
        "**/.temp": true,
        "**/.docusaurus": true,
        "**/.serverless/": true,
        "**/.fusebox/": true,
        "**/.dynamodb/": true,
        "**/.tern-port": true,
        "**/.vscode-test": true,
        ".yarn/cache": true,
        ".yarn/unplugged": true,
        ".yarn/build-state.yml": true,
        ".yarn/install-state.gz": true,
        "**/.pnp.\\*": true,
        "**/.dev.vars": true,
        "**/.wrangler/": true
    }
}